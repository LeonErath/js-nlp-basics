{
	"name": "verify",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next",
		"build": "next build",
		"start": "next start",
		"type-check": "tsc",
		"lint": "eslint --fix"
	},
	"dependencies": {
		"@ant-design/icons": "^4.3.0",
		"@auth0/auth0-react": "^1.2.0",
		"@now/node": "^1.8.5",
		"@tensorflow/tfjs": "^2.8.2",
		"@testing-library/jest-dom": "^5.11.8",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^8.1.0",
		"@types/jest": "^26.0.20",
		"@types/mongodb": "^3.5.2",
		"@types/node": "^13.7.0",
		"@types/react": "^16.9.19",
		"@types/react-dom": "^16.9.5",
		"@types/recharts": "^1.8.18",
		"@types/styled-components": "^4.4.3",
		"@vercel/node": "^1.8.5",
		"@zeit/next-css": "^1.0.1",
		"antd": "^4.10.0",
		"aws-sdk": "^2.824.0",
		"axios": "^0.21.1",
		"classnames": "^2.2.6",
		"compromise": "^13.8.0",
		"dotenv": "^8.2.0",
		"gh-pages": "^2.2.0",
		"husky": "^4.3.6",
		"mongodb": "^3.6.3",
		"next": "^9.3.2",
		"prettier": "^2.2.1",
		"react": "^16.12.0",
		"react-dom": "^16.13.0",
		"react-lottie": "^1.2.3",
		"react-scripts": "3.4.0",
		"recharts": "^1.8.5",
		"slate": "^0.57.1",
		"slate-react": "^0.57.1",
		"styled-components": "^4.4.1",
		"typescript": "~3.8.3"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^4.12.0",
		"@typescript-eslint/parser": "^4.12.0",
		"eslint": "^7.17.0",
		"eslint-plugin-react": "^7.22.0",
		"lint-staged": "^10.5.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx}": [
			"eslint --fix"
		],
		"*.{json,css,md}": [
			"prettier --write"
		]
	},
	"babel": {
		"env": {
			"development": {
				"presets": [
					"next/babel"
				],
				"plugins": [
					[
						"styled-components",
						{
							"ssr": true,
							"displayName": true
						}
					]
				]
			},
			"production": {
				"presets": [
					"next/babel"
				],
				"plugins": [
					[
						"styled-components",
						{
							"ssr": true,
							"displayName": false
						}
					]
				]
			}
		}
	}
}
